{
  "id": null,
  "title": "JustIngredients Bot - Overview",
  "tags": [
    "justingredients",
    "telegram",
    "bot"
  ],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(requests_total[5m])",
          "legendFormat": "{{method}} {{status}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Error Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(requests_total{status=~\"5..\"}[5m]) / rate(requests_total[5m]) * 100",
          "legendFormat": "Error Rate %"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Response Time (95th percentile)",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Telegram Messages Processed",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(telegram_messages_total[5m])",
          "legendFormat": "{{type}} messages"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "Database Operations",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(db_operations_total[5m])",
          "legendFormat": "{{operation}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 6,
      "title": "Database Operation Duration",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(db_operation_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    },
    {
      "id": 7,
      "title": "Circuit Breaker State",
      "type": "stat",
      "targets": [
        {
          "expr": "circuit_breaker_state",
          "legendFormat": "Circuit Breaker"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 8,
      "title": "Active Connections",
      "type": "stat",
      "targets": [
        {
          "expr": "up",
          "legendFormat": "Service Status"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 24
      }
    },
    {
      "id": 9,
      "title": "Memory Usage",
      "type": "graph",
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "legendFormat": "Memory (MB)"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      }
    }
  ],
