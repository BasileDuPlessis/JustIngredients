{
  "id": null,
  "title": "JustIngredients Bot - OCR Performance",
  "tags": [
    "justingredients",
    "ocr",
    "performance"
  ],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "OCR Operations Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(ocr_operations_total[5m])",
          "legendFormat": "{{result}} operations"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "OCR Success Rate",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(ocr_operations_total{result=\"success\"}[5m]) / rate(ocr_operations_total[5m]) * 100",
          "legendFormat": "Success Rate %"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "OCR Processing Time (95th percentile)",
      "type": "graph",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(ocr_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "OCR Image Size Distribution",
      "type": "histogram",
      "targets": [
        {
          "expr": "ocr_image_size_bytes",
          "legendFormat": "Image sizes"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "OCR Error Rate by Type",
      "type": "graph",
      "targets": [
        {
          "expr": "rate(ocr_operations_total{result=\"failure\"}[5m])",
          "legendFormat": "OCR Failures"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 6,
      "title": "Average OCR Processing Time",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(ocr_duration_seconds_sum[5m]) / rate(ocr_duration_seconds_count[5m])",
          "legendFormat": "Average duration"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 7,
      "title": "OCR Throughput (images/minute)",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(ocr_operations_total[5m]) * 60",
          "legendFormat": "Images/minute"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 24
      }
    },
    {
      "id": 8,
      "title": "Largest Image Processed",
      "type": "stat",
      "targets": [
        {
          "expr": "max_over_time(ocr_image_size_bytes[1h])",
          "legendFormat": "Max image size (bytes)"
        }
      ],
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 24
      }
    },
    {
      "id": 9,
      "title": "OCR Queue Depth",
      "type": "graph",
      "targets": [
        {
          "expr": "ocr_queue_depth",
          "legendFormat": "Queue depth"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 28
      }
    },
    {
      "id": 10,
      "title": "Memory Usage During OCR",
      "type": "graph",
      "targets": [
        {
          "expr": "process_resident_memory_bytes / 1024 / 1024",
          "legendFormat": "Memory (MB)"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 28
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "refresh": "30s",
  "schemaVersion": 27,
  "version": 0,
  "links": []
}
